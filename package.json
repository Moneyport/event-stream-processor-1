  "description": "event stream processor to monitor kafka event topic and create traces and store logs to efk stack with APM",
  "main": "app.js",
  "dependencies": {
    "@mojaloop/central-services-error-handling": "7.4.5",
    "@mojaloop/central-services-shared": "7.5.0",
    "@mojaloop/central-services-stream": "7.4.2",
    "@mojaloop/elastic-apm-node": "6.4.0-snapshot",
    "@mojaloop/elastic-apm-node-opentracing": "6.4.1-snapshot",
    "@mojaloop/event-sdk": "7.4.4-snapshot",
    "@mojaloop/opentracing": "6.4.0-snapshot",
    "camelcase": "5.3.1",
    "check-types": "10.0.0",
    "config": "3.2.2",
    "deserialize-error": "0.0.3",
    "elasticsearch": "16.3.0",
    "fluent-logger": "3.3.1",
    "fluent-logger-stream": "0.0.1",
    "healthcheck-server": "1.0.0",
    "json-rules-engine": "4.0.0",
    "lazy-ass": "1.6.0",
    "lodash": "4.17.15",
    "mustache": "3.0.3",
    "request": "2.88.0",
    "request-promise-native": "1.0.7",
    "rxjs": "6.5.3",
    "rxjs-compat": "6.5.3",
    "serialize-error": "^4.1.0",
    "tape": "4.11.0",
    "tapes": "4.1.0",
    "traceparent": "1.0.0",
    "uuid4": "1.1.4"
  },
  "pre-commit": [
    "dep:check",
    "standard",
    "test:unit"
  ],
  "scripts": {
    "start": "WITH_SASL=0&&LD_LIBRARY_PATH=$PWD/node_modules/node-rdkafka/build/deps&& node app.js",
    "test": "npm run test:unit | faucet",
    "test:unit": "tape 'test/unit/**/*.test.js'",
    "test:xunit": "tape 'test/unit/**/*.test.js' | tap-xunit",
    "test:coverage": "istanbul cover tape -- 'test/unit/**/*.test.js'",
    "test:coverage-check": "npm run test:coverage && istanbul check-coverage",
    "standard": "standard",
    "dep:check": "npx ncu -e 2",
    "dep:update": "npx ncu -u"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mojaloop/email-notifier"
  },
  "author": "Valentin Genev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mojaloop/email-notifier"
  },
  "homepage": "https://github.com/mojaloop/email-notifier#readme",
  "devDependencies": {
    "faucet": "0.0.1",
    "jsonpath": "1.0.2",
    "nodemailer-mock": "1.4.3",
    "npm-check-updates": "3.1.21",
    "npm-run-all": "4.1.5",
    "nyc": "14.1.1",
    "pre-commit": "1.2.2",
    "proxyquire": "2.1.3",
    "rewire": "4.0.1",
    "rxjs-marbles": "5.0.2",
    "sinon": "7.4.2",
    "standard": "14.1.0",
    "tape": "4.11.0",
    "tapes": "4.1.0"
  }
